{
  "name": "fleet-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --exec node -r dotenv/config src/server.js",
    "start": "node -r dotenv/config src/server.js",
    "seed": "node -r dotenv/config prisma/seed.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "bcryptjs": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.11.0"
  },
  "engines": {
    "node": "18.x"
  }
}